{
    "manifest_version": 2,
    "name": "Redlines",
    "version": "1.0",
    "description": "Fake news reporter",
    "icons": {
        "128": "images/icon128.png",
        "48": "images/icon48.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Readlines",
        "default_icon": "images/icon128.png"
    },
    "content_scripts": [{
        "css": ["css/banner.css"],
        "js": ["jquery-3.2.1.min.js", "js/banner.js"],
        "matches": ["https://www.nytimes.com/*"]
    }],
    "permissions": ["tabs"],
    "web_accessible_resources": ["css/background-fake.png"]
}